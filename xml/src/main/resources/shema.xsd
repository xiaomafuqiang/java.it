<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.w3school.com.cn"
           xmlns="http://www.w3school.com.cn"
           elementFormDefault="qualified">

    <xs:element sname="note">
        <xs:complexType>
            <xs:sequence>
                <xs:element sname="to" type="xs:string"/>
                <xs:element sname="from" type="xs:string"/>
                <xs:element sname="heading" type="xs:string"/>
                <xs:element sname="body" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>